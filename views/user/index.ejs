<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../partials/header') %>
</head>
<body>
    <h1>All Users Page (no-auth)</h1>
    <ul>
        <li>
            <a href="/users/newUser">Dont see your username, make new</a>
        </li>
        <% users.forEach(user => { %>
        <li>
          <a href="/users/<%= user._id %>"> <%= user.username %> </a>
        </li>
        <% }) %>
      </ul>
      <% if(users.length === 0) { %>
      <p>There are no users yet.</p>
      <% } %>
    <footer>
        <%- include('../partials/footer') %>
    </footer>
</body>
</html>